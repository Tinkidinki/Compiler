class Program{
    int main(){
        int x;
        int y;

        x = 0;
        y = 7;
        
        if ($x > $y){
            callout("printf", "if");
            callout("printf", "if again");
        }
        else {
            callout("printf", "else");
        }
        callout("printf", "always");
        return 0;
    }
}

