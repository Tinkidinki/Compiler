class Program{
    int p;

    int power(int i, int k){
        int ans;
        ans = 1;
        while($k>0){
            ans = $ans*$i;
            k = $k+(-1);
        }
        return $ans;
    }

    int sum(int n, int k){
        int ans;
        ans = 0;
        while($n>0){
            ans = $ans + callout("power", $n, $k);
            n = $n+(-1);
        }
        return $ans;
    }

    void main(){
        p = callout("sum", 3, 3);
        callout("printf", $p);
    }

}